<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="mrp_workorder_finish_view_form" model="ir.ui.view">
        <field name="name">mrp.workorder.finish.wizard.form</field>
        <field name="model">mrp.workorder.finish.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <field name="text"/>
                    <footer>
                        <button name="button_generate_assign" string="Check Availability"
                                    type="object" class="oe_highlight" />
                        or
                        <button name="button_generate_unreserve" string="Unreserve"
                                    type="object" class="oe_highlight" />
                        or
                        <button special="cancel" string="Cancel" class="oe_link" />
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
