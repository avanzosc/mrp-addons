<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
        <record id="view_move_picking_tree_inh_product_brand_code" model="ir.ui.view">
            <field name="name">view.move.picking.tree.inh.product.brand.code</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="jvv_custom.view_move_picking_tree_inh_jvv" />
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <field name="brand_supplier" />
                    <field name="product_brand_code" />
                </field>
            </field>
        </record>
        <record id="view_move_tree_inh_product_brand_code" model="ir.ui.view">
            <field name="name">view.move.tree.view.inh.product.brand.code</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="jvv_custom.view_move_tree_inh_jvv" />
            <field name="arch" type="xml">
                <field name="product_internal_reference" position="before">
                    <field name="brand_supplier" />
                    <field name="product_brand_code" />
                </field>
            </field>
        </record>
        <record id="view_move_search_inh_product_brand_code" model="ir.ui.view">
            <field name="name">view.move.search.inh.product.brand.code</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_search" />
            <field name="arch" type="xml">
                <field name="product_id" position="after">
                    <field name="brand_supplier" />
                    <field name="product_brand_code" />
                </field>
                <filter name="by_product" position="after">
                    <filter
            name="groupby_product_brand_supplier"
            string="Fabricator"
            icon="terp-accessories-archiver"
            domain="[]"
            context="{'group_by' : 'brand_supplier'}"
          />
                    <filter
            string="Product brand code"
            name="by_product_brand_code"
            icon="terp-accessories-archiver"
            domain="[]"
            context="{'group_by':'product_brand_code'}"
          />
                </filter>
            </field>
        </record>
</odoo>
