<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
        <record id="view_picking_form_inh_product_brand_code" model="ir.ui.view">
            <field name="name">view.picking.form.inh.product.brand.code</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field name="arch" type="xml">
                <xpath
          expr="//field[@name='pack_operation_ids']/tree/field[@name='product_id']"
          position="after"
        >
                    <field name="brand_supplier" />
                    <field name="product_brand_code" />
                </xpath>
            </field>
        </record>

</odoo>
