#. module: mrp_production_historical_follower
#: model:mail.template,body_html:mrp_production_historical_follower.mrp_production_histo_bom2
msgid ""
"\n"
"    <div style=\"font-family: 'Lucida Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
