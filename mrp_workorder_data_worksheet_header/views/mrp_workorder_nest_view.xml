<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="mrp_workorder_nest_view_form" model="ir.ui.view">
        <field name="model">mrp.workorder.nest</field>
        <field name="inherit_id" ref="mrp_workorder_grouping_by_material.mrp_workorder_nest_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='nested_line_ids']/tree//field[@name='working_state']" position="after">
                <button name="show_worksheet"
                        type="object"
                        string="Print"
                        icon="fa-print" />
            </xpath>
        </field>
    </record>

</odoo>
