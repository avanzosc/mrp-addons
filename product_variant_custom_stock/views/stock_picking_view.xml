<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="stock_picking_version_form_view">
            <field name="name">stock.picking.version.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_id']" position="after">
                    <field name="product_version_id" />
                    <field name="manual_product_version_id" domain="[('product_id', '=', product_id)]"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
