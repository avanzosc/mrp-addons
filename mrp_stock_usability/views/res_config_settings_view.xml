<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="model">res.config.settings</field>
        <field name="priority" eval="35" />
        <field name="inherit_id" ref="mrp.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@id='mrp_byproduct']/.." position="after">
                <div class="row mt16 o_settings_container">
                    <div
            class="col-lg-6 col-12 o_setting_box"
            id="mrp_stock_usability"
            title=""
          >
                        <div class="o_setting_left_pane">
                            <field name="group_mrp_stock_usability" />
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="group_mrp_stock_usability" />
                            <div class="text-muted">

                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
